{
  "name": "stinky-bot",
  "version": "0.0.1",
  "description": "Semi-declarative Discord Bot for Ruse Cruise",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-ts": "tsc",
    "lint-ts": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "clean-dist": "rm -rf dist",
    "postinstall": "npm run build-ts && npm run lint-ts",
    "move-scripts": "cp -R src/scripts dist/scripts && mkdir -p dist/__temp && chmod +x dist/scripts/*",
    "build": "npm run clean-dist && npm run postinstall",
    "start": "node dist/bot.js",
    "migrate": "node dist/scripts/migrate.js"
  },
  "author": "Pablo Morales",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^7.1.0",
    "eslint-plugin-functional": "^1.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.0",
    "@types/cron": "^1.7.2",
    "@types/mathjax": "0.0.36",
    "@types/node": "^12.19.12",
    "@types/pg": "^7.14.7",
    "@types/wav": "^1.0.0",
    "@types/ws": "^6.0.4",
    "aws-sdk": "^2.824.0",
    "axios": "^0.21.1",
    "chrono-node": "^2.2.4",
    "cron": "^1.8.2",
    "discord.js": "^14.9.0",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.4.1",
    "firebase-admin": "^9.4.2",
    "markovchain": "^1.0.2",
    "mathjax-full": "^3.1.2",
    "pg-promise": "^10.5.6",
    "prism-media": "^2.0.0-alpha.0",
    "sodium-native": "^3.4.1"
  }
}
